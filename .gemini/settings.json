{
  "mcpServers": {
    "Playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "GitHub": {
      "type": "sse",
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "Gemini": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-gemini"
      ],
      "envFile": "C:/powershell/modules/mcp-powershell-server/.env"
    },
    "powershell-https": {
      "command": "pwsh",
      "args": [
        "-File",
        "C:/powershell/modules/mcp-powershell-server/src/servers/mcp-powershell-https.ps1"
      ],
      "envFile": "C:/powershell/modules/mcp-powershell-server/.env"
    },
    "powershell-stdio": {
      "command": "pwsh",
      "args": [
        "-File",
        "C:/powershell/modules/mcp-powershell-server/src/servers/mcp-powershell-stdio.ps1"
      ],
      "envFile": "C:/powershell/modules/mcp-powershell-server/.env"
    },
    "wordpress-cli": {
      "command": "pwsh",
      "args": [
        "-NoProfile",
        "-ExecutionPolicy",
        "Bypass",
        "-File",
        "C:/powershell/modules/mcp-powershell-server/src/servers/mcp-powershell-wordpress.ps1"
      ],
      "envFile": "C:/powershell/modules/mcp-powershell-server/.env"
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "envFile": "C:/powershell/modules/mcp-powershell-server/.env"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "pydoll": {
      "command": "python",
      "args": [
        "-m",
        "pydoll_mcp.server"
      ],
      "envFile": "C:/powershell/modules/mcp-powershell-server/.env"
    },
    "Hugging-Face": {
      "type": "sse",
      "url": "https://huggingface.co/mcp?login"
    }
  }
}